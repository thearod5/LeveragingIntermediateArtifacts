Open(
	"/Users/albertorodriguez/Projects/LeveragingIntermediateArtifacts/graphs/sampling_tables/rq1/rq1_sampling.jmp"
);
Graph Builder(
	Size( 1004, 704 ),
	Show Legend( 0 ),
	Variables(
		X( :Percent of Intermediate Artifacts Used ),
		Y( :Value ),
		Group X( :Metric ),
		Group Y( :Dataset ),
		Color( :Percent of Intermediate Artifacts Used )
	),
	Elements( Points( X, Y, Legend( 17 ) ), Line Of Fit( X, Y, Legend( 19 ) ) ),
	SendToReport(
		Dispatch(
			{},
			"Percent of Intermediate Artifacts Used",
			ScaleBox,
			{Format( "Percent", 12, 0 ), Min( 0 ), Max( 1 ), Inc( 0.1 ),
